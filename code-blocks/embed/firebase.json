{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      { "source": "/", "function": "index" },
      { "source": "/embed", "destination": "embed.html" },
      { "source": "/oembed", "function": "oembed" }
    ]
  }
}
